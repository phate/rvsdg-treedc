// first example graph in https://en.wikipedia.org/wiki/Tree_decomposition

graph g {

    { rank = same; 0; 1; 5; }
    { rank = same; 2; 6; }
    { rank = same; 3; 4; 7; }

    0 -- { 1 2 } // a
    1 -- { 4 5 6 } // b
    2 -- { 1 3 4 } // c
    3 -- { 4 } // d
    4 -- { 6 7 } // e
    5 -- { 6 } // f
    6 -- { 7 } // g
    7 -- { } // h
}
