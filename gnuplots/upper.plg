#!/usr/bin/gnuplot -persist

GNUTERM = "x11"

load '~/gnuplot-colorbrewer/qualitative/Dark2.plt'

# ================================== Error ==================================

set auto x
set yrange [0:10]

set boxwidth 0.8 absolute
set style fill solid 1 border 0
unset key

set style data histograms
set ylabel 'Upper treewidth bound'
set xtics rotate
set offset 1,1,1
set grid ytics
plot 'upper.results' u ($1):3:($0):xtic(2) w boxes lc variable
